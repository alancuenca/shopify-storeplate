{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/layouts/components/*"],
      "@/functional-components/*": ["./src/layouts/functional-components/*"],
      "@/partials/*": ["./src/layouts/partials/*"],
      "@/helpers/*": ["./src/layouts/helpers/*"],
      "@/lib/*": ["./src/lib/*"],
      "@/types/*": ["./src/types/*"]
    }
  }
}